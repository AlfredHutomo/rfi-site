{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "program_name": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Advanced"
      ],
      "default": "Beginner"
    },
    "age_range": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "program_name"
    },
    "tags": {
      "type": "component",
      "repeatable": false,
      "component": "program.program-tag"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "sections.block-content",
        "sections.hero",
        "sections.our-programs",
        "sections.page-banner",
        "sections.testimonials",
        "sections.faq",
        "sections.icon-text-link-block",
        "sections.link-group",
        "sections.register-interest",
        "sections.sponsors",
        "sections.statistic-block",
        "sections.text-block",
        "sections.text-image-block"
      ]
    }
  }
}
