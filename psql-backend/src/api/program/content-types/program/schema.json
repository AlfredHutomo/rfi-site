{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Advanced"
      ],
      "default": "Beginner"
    },
    "age_range": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "sections.banner",
        "sections.block-content",
        "sections.faq",
        "sections.grid-about-us",
        "sections.hero",
        "sections.icon-text-link-group",
        "sections.icon-text-tile",
        "sections.link-group",
        "sections.our-programs",
        "sections.page-banner",
        "sections.people-highlight-block",
        "sections.register-interest",
        "sections.sponsors-details",
        "sections.sponsors",
        "sections.statistic-block",
        "sections.testimonials",
        "sections.text-block",
        "sections.text-image-block",
        "sections.text-tile-row-block",
        "sections.timeline-block",
        "sections.usp-banner"
      ]
    },
    "venue": {
      "type": "component",
      "repeatable": false,
      "component": "program.program-venue"
    },
    "schedule": {
      "type": "component",
      "repeatable": false,
      "component": "program.program-schedule"
    },
    "disclamer": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::program-tag.program-tag",
      "mappedBy": "programs"
    },
    "coaches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::rfi-team-member.rfi-team-member",
      "inversedBy": "coaches"
    }
  }
}
