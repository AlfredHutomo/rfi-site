{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "shortName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "shortName"
    },
    "seo": {
      "displayName": "SEO",
      "type": "component",
      "repeatable": false,
      "component": "elements.seo"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "sections.hero",
        "sections.our-programs",
        "sections.testimonials",
        "sections.block-content",
        "sections.page-banner",
        "sections.text-image-block",
        "sections.text-block",
        "sections.statistic-block",
        "sections.link-group",
        "sections.sponsors",
        "sections.faq",
        "sections.register-interest",
        "sections.usp-banner",
        "sections.people-highlight-block",
        "sections.grid-about-us",
        "sections.icon-text-link-group",
        "sections.icon-text-tile",
        "sections.banner",
        "sections.timeline-block",
        "sections.sponsors-details",
        "sections.text-tile-row-block",
        "sections.blog-lists",
        "layout.contact-us",
        "layout.register"
      ]
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parent"
    },
    "indexPage": {
      "type": "boolean",
      "default": false
    }
  }
}
